class Solution {
    public boolean isPalindrome(int x) {
        String XString = String.valueOf(x);
        for (int i=0; i<XString.length()/2; i++) {
            if (XString.charAt(i)!=XString.charAt(XString.length()-i-1)){
                return false;
            }  
        }
        return true;
    }
}
